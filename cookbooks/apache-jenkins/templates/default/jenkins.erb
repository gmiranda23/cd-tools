<VirtualHost *:<%= @port %>>
  ServerAdmin <%= @server_admin %>
  ServerName <%= @server_name %>
  ServerAlias <%= @server_alias %>
  ProxyRequests Off
  <Proxy *>
      Order deny,allow
      Allow from all
  </Proxy>
  ProxyPreserveHost on
  ProxyPass / http://localhosthost:<%= @proxy_port>/
</VirtualHost>
